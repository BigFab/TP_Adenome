#
# Cleanup & Data Preparation
# Les fichiers ont été transformés en CSV avant import dans R
#
setwd("/home/sougeyf/TP_Adenome")
vapor<-read.csv(file = "datasets/VAPOR.csv", sep = ",")
# Cast des variables booléennes
vapor$Comorbidité.CardioVx<-as.logical(vapor$Comorbidité.CardioVx)
vapor$poteur.de.sonde<-as.logical(vapor$poteur.de.sonde)
vapor$Evenement.H.D<-as.logical(vapor$Evenement.H.D)
vapor$Transfusion.PerO<-as.logical(vapor$Transfusion.PerO)
vapor$caillotage<-as.logical(vapor$caillotage)
vapor$reprise.au.bloc<-as.logical(vapor$reprise.au.bloc)
# Cast des variables catégoriques
vapor$Indication<-as.factor(vapor$Indication)
vapor$Anesthésie<-as.factor(vapor$Anesthésie)
vapor$Evenement.H.D<-as.factor(vapor$Evenement.H.D)
vapor$Technique<-as.factor(vapor$Technique)
# Cast des variables catégoriques ordonnées
vapor$QoL.P.O<-as.factor(vapor$QoL.P.O)
# 20 premières colonnes du dataset -> pré-opératoire
vapor_pre<-vapor[,1:21]
# Dernières colonnes du dataset -> post-opératoire
vapor_post<-vapor[,22:42]
#
# Analyse Exploratoire de Vapor
#
# Boxplots des variables numériques
par(mfrow = c(1,1))
for (i in 1:ncol(vapor_pre)) {
classx= class(vapor_pre[,i])
#x11(display="")
if (classx == "numeric")
boxplot(vapor_pre[,i])
else
barplot(table(vapor_pre[,i]))
title(colnames(vapor_pre)[i])
}
